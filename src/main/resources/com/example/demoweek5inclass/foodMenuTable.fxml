<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demoweek5inclass.FoodMenuTableController">
   <children>
      <BorderPane prefHeight="394.0" prefWidth="594.0">
         <center>
            <TableView fx:id="tableViewFoodMenu" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="colPrice" prefWidth="75.0" text="Price" />
                  <TableColumn fx:id="colSpicyLevel" prefWidth="75.0" text="Spicy Level" />
                  <TableColumn fx:id="colCal" prefWidth="75.0" text="Calorie" />
              </columns>
            </TableView>
         </center>
         <top>
            <Label text="Food Menu" BorderPane.alignment="CENTER" />
         </top>
         <bottom>
            <Label text="Enjoy!" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
